{
    "compilerOptions": {
        "outDir": "./dist",
        "module": "es6",
        "target": "ES5",
        "moduleResolution": "node",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "lib": ["es2015", "dom"],
        "noImplicitAny": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "exclude": [
        "node_modules",
        "wwwroot",
        "typings"
    ]
}